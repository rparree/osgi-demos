<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="org.demo.osgi.datasource.jpa.AirportEntity" >
        <table name="airport" schema="jbtravel" catalog="postgres" />
        <attributes>
            <id name="airportid">
                <column name="airportid"/>
            </id>
            <basic name="airportname">
                <column name="airportname"/>
            </basic>
            <basic name="airportcode">
                <column name="airportcode"/>
            </basic>

        </attributes>
    </entity>
</entity-mappings>
